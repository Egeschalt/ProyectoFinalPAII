<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<title>@ViewData["Title"] - ProyectoFinalPAII</title>
	<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css" rel="stylesheet">
	<link href="https://getbootstrap.com/docs/5.3/assets/css/docs.css" rel="stylesheet">
	<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css">
	<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js"></script>
	<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
	<link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
	<link rel="stylesheet" type="text/css" href="~/css/Estilografia.css">
	<link rel="stylesheet" href="~/ProyectoFinalPAII.styles.css" asp-append-version="true" />
</head>
<body>

	<div class="container">
		<main role="main" class="pb-3">
			@RenderBody()
		</main>
	</div>

	<footer style="background-color: white; color: white; text-align: center; padding: 1px;" class="border-top footer text-muted text-center">
		<div class="container text-center">
			&copy; 2023 - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
			<a>Derechos reservados por la escuela académica de Ingeniería de Sistemas</a>
			<a href="https://www.unc.edu.pe/" target="_blank">
				<img src="~/img/unc.png" height="50px" class="mx-4" />
			</a>
			<a  asp-controller="Home" asp-action="Index">Volver</a>

		</div>
	</footer>
	<script src="~/lib/jquery/dist/jquery.min.js"></script>
	<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
	<script src="~/js/site.js" asp-append-version="true"></script>
	@await RenderSectionAsync("Scripts", required: false)
</body>
</html>
